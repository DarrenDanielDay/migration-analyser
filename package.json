{
	"name": "vscode-extension-demo",
	"displayName": "vscode-extension-demo",
	"description": "Just a demo for learning vscode.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-extension-demo.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-extension-demo.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vscode-extension-demo.test",
				"title": "My Test"
			},
			{
				"command": "vscode-extension-demo.demo",
				"title": "Reference Demo"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"lint:types": "tsc --noEmit",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@types/jquery": "^3.5.5",
		"@types/lodash": "^4.14.168",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "^7.3.4",
		"jsonc-parser": "^3.0.0",
		"lodash": "^4.17.21",
		"rimraf": "^3.0.2",
		"semver": "^7.3.4",
		"typedoc": "^0.20.32",
		"typescript-vscode-sh-plugin": "^0.6.14",
		"vscode-extension-telemetry": "^0.1.7",
		"vscode-nls": "^5.0.0"
	}
}
